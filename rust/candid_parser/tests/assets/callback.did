type Fn = func(nat) -> (nat) query;

type RInline = record {
    fn : func(nat) -> (nat) query;
    x : nat;
};
type R = record {
    fn : Fn;
    x : nat;
};
service : {
    add_two : (nat) -> (nat);

    high_order_fn : (nat, Fn) -> (nat);
    high_order_fn_inline : (nat, func(nat) -> (nat) query) -> (nat);
    high_order_fn_via_record : (R) -> (nat);
    high_order_fn_via_record_inline : (RInline) -> (nat);

    fn : Fn;
    inline_fn : (nat) -> (nat);
};
